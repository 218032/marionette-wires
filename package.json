{
  "name": "marionette-browserify-starter-kit",
  "version": "0.0.0",
  "description": "Starter kit for Marionette.js Applications using Browserify",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/thejameskyle/marionette-browserify-starter-kit.git"
  },
  "keywords": [
    "marionette",
    "browserify",
    "backbone"
  ],
  "author": "James Kyle <me@thejameskyle.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thejameskyle/marionette-browserify-starter-kit/issues"
  },
  "homepage": "https://github.com/thejameskyle/marionette-browserify-starter-kit",
  "devDependencies": {
    "grunt-contrib-clean": "^0.5.0",
    "grunt-concurrent": "^0.5.0",
    "grunt": "^0.4.4",
    "browserify": "^3.44.0",
    "grunt-browserify": "^2.0.7",
    "grunt-contrib-jshint": "^0.10.0",
    "load-grunt-tasks": "^0.4.0",
    "browserify-shim": "^3.4.1",
    "grunt-contrib-connect": "^0.7.1",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-contrib-copy": "^0.5.0",
    "deglobalify": "^0.2.0",
    "grunt-contrib-less": "^0.11.0",
    "grunt-autoprefixer": "^0.7.2"
  },
  "dependencies": {
    "jquery": "^2.1.0",
    "underscore": "^1.6.0",
    "backbone": "^1.1.2",
    "backbone.marionette": "1.8.0"
  },
  "browser": {
    "jquery": "./node_modules/jquery/dist/jquery.js",
    "underscore": "./node_modules/underscore/underscore.js",
    "backbone": "./node_modules/backbone/backbone.js"
  },
  "browserify-shim": {
    "jquery": "$",
    "underscore": "_",
    "backbone": {
      "depends": [
        "underscore"
      ],
      "exports": "Backbone"
    }
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "deglobalify"
    ]
  }
}
